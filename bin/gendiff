#!/usr/bin/env php

<?php

require_once __DIR__ . '/../src/autoload.php';

$doc = <<<DOC
Generate diff

Usage: gendiff
  gendiff (-h|--help)
  gendiff (-v|--version)

Options:
  -h --help                     Show this screen
  -v --version                  Show version
DOC;

$args = (new \Docopt\Handler)->handle($doc);
